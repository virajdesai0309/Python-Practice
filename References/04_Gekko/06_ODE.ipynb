   "source": [
    "import numpy as np\n",
    "from gekko import GEKKO\n",
    "\n",
    "tm = np.linspace(0,20,100)\n",
    "m = GEKKO(remote=False)\n",
    "m.time = tm\n",
    "k = 10\n",
    "y = m.Var(value=5.0)\n",
    "t = m.Param(value=tm)\n",
    "m.Equation(y.dt() == -k*y)\n",
    "m.options.IMODE = 4\n",
    "m.solve(disp=True)"
   ]
